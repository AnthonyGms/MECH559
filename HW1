      "   \n",
      "   Training ...\n",
      "   Training - done. Time (sec):  0.0331333\n",
      "___________________________________________________________________________\n",
      "   \n",
      " Evaluation\n",
      "   \n",
      "      # eval points. : 101\n",
      "   \n",
      "   Predicting ...\n",
      "   Predicting - done. Time (sec):  0.0002236\n",
      "   \n",
      "   Prediction time/pt. (sec) :  0.0000022\n",
      "   \n"
     ]
    },
    {
     "data": {
      "image/png": "",
      "text/plain": [
       "<Figure size 640x480 with 1 Axes>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "from smt.surrogate_models import KRG\n",
    "\n",
    "sm = KRG(theta0=[1e-2],corr=\"squar_exp\",poly=\"constant\")\n",
    "sm.set_training_values(x1, pexp)\n",
    "sm.train()\n",
    "\n",
    "fSMT = sm.predict_values(x)\n",
    "# estimated variance\n",
    "mse = sm.predict_variances(x)\n",
    "\n",
    "fig, axs = plt.subplots(1)\n",
    "# add a plot with variance\n",
    "axs.plot(x1, pexp, \"bo\")\n",
    "axs.plot(x, fSMT, \"-r\")\n",
    "axs.fill_between(\n",
    "    np.ravel(x),\n",
    "    np.ravel(fSMT - 3 * np.sqrt(mse)),\n",
    "    np.ravel(fSMT + 3 * np.sqrt(mse)),\n",
    "    color=\"lightgrey\",\n",
    ")\n",
    "axs.set_xlabel(\"$x_1$\")\n",
    "axs.set_ylabel(\"$p_{exp}$\")\n",
    "axs.legend(\n",
    "    [\"Training data\", \"Prediction\", \"Confidence Interval 99%\"],\n",
    "    loc=\"lower right\",\n",
    ")\n",
    "plt.show()"
   ]
  },
