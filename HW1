   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "# (2) Multi-dimensional Kriging using `smt`"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "",
      "text/plain": [
       "<Figure size 640x480 with 1 Axes>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "from matplotlib import cm\n",
    "\n",
    "data = np.genfromtxt('data1.csv', delimiter=',', skip_header=1)\n",
    "S = data[:,:2]\n",
    "Y = data[:,2:]\n",
    "\n",
    "sm = KRG(theta0=[0.1,],corr=\"squar_exp\",poly=\"constant\", print_global=False)\n",
    "sm.set_training_values(S, Y)\n",
    "sm.train()\n",
    "\n",
    "X1,X2 = np.meshgrid(np.linspace(0,100,40),np.linspace(0,100,40))\n",
    "X = np.hstack((X1.reshape([-1,1]),X2.reshape([-1,1]))) # flatten grids and place them side-by-side\n",
    "\n",
    "fSMT = sm.predict_values(X)\n",
    "# estimated variance\n",
    "mse = sm.predict_variances(X)\n",
    "\n",
    "fig, axs = plt.subplots(1,subplot_kw={\"projection\": \"3d\"})\n",
    "# plot\n",
    "axs.plot_surface(X1, X2, fSMT.reshape(X1.shape),cmap=cm.coolwarm,linewidth=0, antialiased=False, alpha=0.5)\n",
    "axs.scatter(S[:,0], S[:,1], Y, marker=\".\", color='k', linestyle=\"None\")\n",
    "axs.view_init(elev=30., azim=210)\n",
    "axs.set_xlabel(\"$x_1$\")\n",
    "axs.set_ylabel(\"$x_2$\")\n",
    "axs.set_zlabel(r\"$\\hat{y}$\");"
   ]
  },
